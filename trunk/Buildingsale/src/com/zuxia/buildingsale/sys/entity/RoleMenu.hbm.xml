<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zuxia.buildingsale.sys.entity.RoleMenu" table="role_menu" catalog="loupan">
        <id name="rmId" type="java.lang.Integer">
            <column name="rm_id" />
            <generator class="native" />
        </id>
        <many-to-one name="menuInfo" class="com.zuxia.buildingsale.sys.entity.MenuInfo" fetch="select" lazy="false">
            <column name="menu_id" not-null="true" unique="true">
                <comment>目录编号</comment>
            </column>
        </many-to-one>
        <many-to-one name="roleInfo" class="com.zuxia.buildingsale.sys.entity.RoleInfo" fetch="select" lazy="false">
            <column name="role_id" not-null="true" unique="true">
                <comment>角色编号</comment>
            </column>
        </many-to-one>
    </class>
</hibernate-mapping>
